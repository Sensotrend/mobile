{
  "name": "tidepool-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "pre": "cp crna-entry.js index.js",
    "prestorybook": "cp storybook-entry.js index.js && rnstl",
    "test": "node node_modules/jest/bin/jest.js",
    "format": "prettier-eslint \"src/**/*.js\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "axios": "0.18.0",
    "date-fns": "1.30.1",
    "expo": "^31.0.4",
    "expo-three": "3.0.0-alpha.8",
    "expokit": "31.0.2",
    "react": "16.5.0",
    "glamorous-native": "1.4.2",
    "prop-types": "15.7.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-native-device-info": "0.26.4",
    "react-native-walkthrough-tooltip": "^0.2.0",
    "react-navigation": "2.0.4",
    "react-navigation-redux-helpers": "1.1.2",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "rollbar-react-native": "0.5.0",
    "three": "0.101.1",
    "three-bmfont-text": "2.3.0",
    "twitter-text": "3.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "4.1.12",
    "@storybook/addon-ondevice-knobs": "4.1.12",
    "@storybook/addon-storyshots": "4.1.12",
    "@storybook/addons": "4.1.12",
    "@storybook/react-native": "4.1.12",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-preset-expo": "5.0.0",
    "babel-runtime": "6.26.0",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-import-resolver-reactnative": "1.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-native": "3.6.0",
    "faker": "4.1.0",
    "flow-bin": "0.93.0",
    "jest-expo": "^31.0.0",
    "patch-package": "6.0.2",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.16.4",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "react-dom": "16.3.2",
    "react-devtools": "3.6.0",
    "react-native-debugger-open": "0.3.18",
    "react-native-storybook-loader": "1.8.0",
    "react-test-renderer": "16.3.2"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./__stories__"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    },
    "setupTestFrameworkScriptFile": "./jest-setupTestFramework.js"
  }
}
